{
  "name": "waitless",
  "version": "1.0.0",
  "description": "Waitless Queue Management System",
  "main": "dist/server/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"blue,green\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "nuxt dev",
    "dev:backend": "tsx watch src/server/index.ts",
    "build": "tsc --project tsconfig.server.json && nuxt build",
    "build:frontend": "nuxt build",
    "build:backend": "tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "start:frontend": "nuxt preview",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:status": "sequelize-cli db:migrate:status",
    "migrate:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "seed:reset": "sequelize-cli db:seed:undo:all && sequelize-cli db:seed:all",
    "db:reset": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "db:fresh": "npm run seed:undo && npm run migrate:undo:all && npm run migrate && npm run seed",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "job:summary": "tsx src/server/jobs/daily-summary.job.ts",
    "typecheck": "./scripts/check-ts.sh",
    "typecheck:frontend": "nuxt typecheck",
    "typecheck:backend": "tsc --project tsconfig.server.json --noEmit",
    "typecheck:vue": "vue-tsc --noEmit",
    "check:all": "npm run typecheck && npm run lint",
    "fix:all": "npm run lint:fix && npm run typecheck"
  },
  "keywords": [
    "queue",
    "management",
    "express",
    "sequelize",
    "typescript"
  ],
  "author": "Waitless Team",
  "license": "MIT",
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "postcss": "^8.5.6",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.19",
    "vue": "^3.3.8",
    "vue-toastification": "^2.0.0-rc.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.6",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/ui": "^1.0.4",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nuxt": "^3.8.2",
    "prettier": "^3.7.4",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "vue-tsc": "^3.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
